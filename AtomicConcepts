public class AtmoicConcepts extends Chemistry
{
    public static void main(String [] args)
    {
    String word = "anathema";
    String a="";
    for(int i=0; i<word.length(); i++)
      {
        if(word.charAt(i) == 'a')
        {
            a+= "2";   
        }
        else
        {
            a+=word.charAt(i);
        }
       }
    System.out.println(a);
    }
}
    

