public class AcidsAndBases extends Chemistry
{
  public AcidsAndBases(){
        super();
    }
    public void makeQuestion(){
        int rando = (int)(Math.random()*3);
        if(rando==0){
              this.printChemQuestions("The pH of a 0.1M CH3COOH solution is ", "greater than 1 but less than 7", "less than 1","equal to 7","greater than 7");
     }
        if(rando==1){
           this.printChemQuestions("What is the pH of a 0.001 M solution of HCL?","3","1","7","11");
     }
        if(rando==2){
         this.printChemQuestions("Acording to the Bronstead-Lowry Theory, an acid is", "a proton donor","an electron donor","a proton acceptor","an electron acceptor");
        }
    }
}
